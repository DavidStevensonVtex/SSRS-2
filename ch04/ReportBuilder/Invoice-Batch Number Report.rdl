  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <CommandText>SELECT c.BillingCity, c.Name, ih.BatchNumber, ih.InvoiceNumber, ih.TotalAmount
FROM Customer c
	LEFT OUTER JOIN InvoiceHeader ih
		ON c.CustomerNumber = ih.CustomerNumber</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="BillingCity">
          <DataField>BillingCity</DataField>
